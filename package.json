{
  "name": "@cvplus/multimedia",
  "version": "2.3.0",
  "description": "CVPlus Multimedia - Phase 2C Complete: Interactive QR components with enhanced backend integration",
  "main": "dist/index.minimal.js",
  "module": "dist/index.minimal.esm.js",
  "types": "dist/index.minimal.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.minimal.esm.js",
      "require": "./dist/index.minimal.js",
      "types": "./dist/index.minimal.d.ts"
    },
    "./frontend": {
      "import": "./dist/frontend/index.esm.js",
      "require": "./dist/frontend/index.js",
      "types": "./dist/frontend/index.d.ts"
    },
    "./integration": {
      "import": "./dist/standalone/frontend/integration/StandaloneIntegration.esm.js",
      "require": "./dist/standalone/frontend/integration/StandaloneIntegration.js",
      "types": "./dist/standalone/frontend/integration/StandaloneIntegration.d.ts"
    },
    "./standalone": {
      "import": "./dist/standalone/frontend/standalone-index.esm.js",
      "require": "./dist/standalone/frontend/standalone-index.js",
      "types": "./dist/standalone/frontend/standalone-index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.esm.js",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./constants": {
      "import": "./dist/constants/index.esm.js",
      "require": "./dist/constants/index.js",
      "types": "./dist/constants/index.d.ts"
    },
    "./services": {
      "import": "./dist/services/index.esm.js",
      "require": "./dist/services/index.js",
      "types": "./dist/services/index.d.ts"
    },
    "./processors": {
      "import": "./dist/processors/index.esm.js",
      "require": "./dist/processors/index.js",
      "types": "./dist/processors/index.d.ts"
    },
    "./storage": {
      "import": "./dist/storage/index.esm.js",
      "require": "./dist/storage/index.js",
      "types": "./dist/storage/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.esm.js",
      "require": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.esm.js",
      "require": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./components/players/AIPodcastPlayer": {
      "import": "./dist/components/players/AIPodcastPlayer.esm.js",
      "require": "./dist/components/players/AIPodcastPlayer.js",
      "types": "./dist/components/players/AIPodcastPlayer.d.ts"
    },
    "./components/generation/VideoIntroduction": {
      "import": "./dist/components/generation/VideoIntroduction.esm.js",
      "require": "./dist/components/generation/VideoIntroduction.js",
      "types": "./dist/components/generation/VideoIntroduction.d.ts"
    },
    "./components/gallery/PortfolioGallery": {
      "import": "./dist/components/gallery/PortfolioGallery.esm.js",
      "require": "./dist/components/gallery/PortfolioGallery.js",
      "types": "./dist/components/gallery/PortfolioGallery.d.ts"
    },
    "./hooks": {
      "import": "./dist/frontend/hooks/useMultimediaFeatures.esm.js",
      "require": "./dist/frontend/hooks/useMultimediaFeatures.js",
      "types": "./dist/frontend/hooks/useMultimediaFeatures.d.ts"
    },
    "./providers": {
      "import": "./dist/frontend/providers/MultimediaProvider.esm.js",
      "require": "./dist/frontend/providers/MultimediaProvider.js",
      "types": "./dist/frontend/providers/MultimediaProvider.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:types": "tsc --project tsconfig.build.json",
    "build:js": "tsc --project tsconfig.js.json",
    "build:frontend": "npm run clean:frontend && npm run build:frontend-types",
    "build:frontend-types": "tsc --project tsconfig.frontend.json",
    "build:simple": "npm run clean:simple && npm run build:simple-types",
    "build:simple-types": "tsc --project tsconfig.simple.json",
    "build:standalone": "npm run clean:standalone && npm run build:standalone-types",
    "build:standalone-types": "tsc --project tsconfig.standalone.json",
    "build:watch": "npm run build:types -- --watch",
    "clean": "rimraf dist",
    "clean:frontend": "rimraf dist/frontend",
    "clean:simple": "rimraf dist/frontend-simple",
    "clean:standalone": "rimraf dist/standalone",
    "dev": "npm run build:watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "type-check": "tsc --noEmit",
    "type-check:frontend": "tsc --project tsconfig.frontend.json --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@cvplus/core": "file:../core",
    "@cvplus/auth": "file:../auth",
    "@cvplus/i18n": "file:../i18n",
    "@ffmpeg/ffmpeg": "^0.12.10",
    "@types/node": "^22.9.1",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.6.0",
    "file-type": "^19.5.0",
    "firebase": "^10.14.1",
    "firebase-admin": "^12.1.0",
    "firebase-functions": "^5.0.1",
    "fluent-ffmpeg": "^2.1.2",
    "image-size": "^1.1.1",
    "lucide-react": "^0.542.0",
    "mime-types": "^2.1.35",
    "music-metadata": "^10.4.0",
    "openai": "^4.0.0",
    "qrcode": "^1.5.3",
    "react": ">=18.0.0",
    "react-intersection-observer": "^9.16.0",
    "react-hot-toast": "^2.4.1",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/jest": "^29.5.14",
    "@types/mime-types": "^2.1.4",
    "@types/qrcode": "^1.5.5",
    "@types/sharp": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.27.4",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "typescript": ">=5.0.0"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "cvplus",
    "multimedia",
    "image-processing",
    "video-processing",
    "audio-processing",
    "file-upload",
    "storage-management",
    "cdn-integration",
    "typescript"
  ],
  "author": "Gil Klainert",
  "license": "PROPRIETARY",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cvplus/cvplus.git",
    "directory": "packages/multimedia"
  },
  "bugs": {
    "url": "https://github.com/cvplus/cvplus/issues"
  },
  "homepage": "https://github.com/cvplus/cvplus/tree/main/packages/multimedia#readme"
}
