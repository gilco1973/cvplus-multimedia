{
  "name": "@cvplus/multimedia",
  "version": "1.0.0",
  "description": "CVPlus Multimedia - Comprehensive media processing, optimization, and storage management",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.esm.js",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./constants": {
      "import": "./dist/constants/index.esm.js",
      "require": "./dist/constants/index.js",
      "types": "./dist/constants/index.d.ts"
    },
    "./services": {
      "import": "./dist/services/index.esm.js",
      "require": "./dist/services/index.js",
      "types": "./dist/services/index.d.ts"
    },
    "./processors": {
      "import": "./dist/processors/index.esm.js",
      "require": "./dist/processors/index.js",
      "types": "./dist/processors/index.d.ts"
    },
    "./storage": {
      "import": "./dist/storage/index.esm.js",
      "require": "./dist/storage/index.js",
      "types": "./dist/storage/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.esm.js",
      "require": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:types && npm run build:rollup",
    "build:types": "tsc --project tsconfig.build.json",
    "build:rollup": "rollup -c",
    "build:watch": "npm run build:types -- --watch",
    "clean": "rimraf dist",
    "dev": "npm run build:watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@cvplus/core": "^1.0.0",
    "@cvplus/auth": "^1.0.0",
    "@types/node": "^22.9.1",
    "sharp": "^0.33.5",
    "@ffmpeg/ffmpeg": "^0.12.10",
    "firebase": "^10.14.1",
    "aws-sdk": "^2.1691.0",
    "mime-types": "^2.1.35",
    "file-type": "^19.5.0",
    "image-size": "^1.1.1",
    "music-metadata": "^10.4.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/jest": "^29.5.14",
    "@types/mime-types": "^2.1.4",
    "@types/sharp": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.27.4",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "cvplus",
    "multimedia",
    "image-processing",
    "video-processing",
    "audio-processing",
    "file-upload",
    "storage-management",
    "cdn-integration",
    "typescript"
  ],
  "author": "Gil Klainert",
  "license": "PROPRIETARY",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cvplus/cvplus.git",
    "directory": "packages/multimedia"
  },
  "bugs": {
    "url": "https://github.com/cvplus/cvplus/issues"
  },
  "homepage": "https://github.com/cvplus/cvplus/tree/main/packages/multimedia#readme"
}